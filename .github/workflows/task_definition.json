{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::865414894512:user/github-ccid-liveclass",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "document-portal-container",
      "image": "865414894512.dkr.ecr.eu-north-1.amazonaws.com/documentportalliveclass",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
