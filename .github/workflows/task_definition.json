        },

        {
          "name": "GOOGLE_API_KEY",
          "valueFrom": "AIzaSyD9PhzqX2DwxETN4rwkvFn1dB0WlRNGzKw"
        }
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "ap-southeast-2",
